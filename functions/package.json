{
  "name": "functions",
  "scripts": {
    "lint": "eslint ./src/index.ts --fix",
    "build": "tsc",
    "mjml": "mjml ./src/booking.mjml -o ./src/booking.html --config.minify true --config.minifyOptions='{\"minifyCSS\": true, \"removeEmptyAttributes\": false}'",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "date-fns": "2.23.0",
    "date-fns-tz": "1.1.6",
    "firebase-admin": "^9.11.0",
    "firebase-functions": "^3.15.3",
    "mjml": "^4.10.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "firebase-functions-test": "^0.3.1",
    "typescript": "4.3.5"
  },
  "private": true
}
