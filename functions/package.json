{
  "name": "functions",
  "scripts": {
    "lint": "eslint ./src/index.ts --fix",
    "build": "tsc",
    "mjml": "mjml ./src/booking.mjml -o ./src/booking.html --config.minify true",
    "serve": "firebase emulators:start",
